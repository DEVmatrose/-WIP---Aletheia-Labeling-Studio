/**
 * Combined mock data for demo
 */
export const MOCK_ALL_DATA: AletheiaItem[] = [
  ...MOCK_TECHNICAL_DATA,
  ...MOCK_PSYCHOLOGICAL_DATA,
  ...MOCK_SCIENTIFIC_DATA
]

/**
 * Get mock data by pillar
 */
export function getMockDataByPillar(pillar: 'technical' | 'psychological' | 'scientific' | 'all'): AletheiaItem[] {
  if (pillar === 'all') return MOCK_ALL_DATA
  if (pillar === 'technical') return MOCK_TECHNICAL_DATA
  if (pillar === 'psychological') return MOCK_PSYCHOLOGICAL_DATA
  if (pillar === 'scientific') return MOCK_SCIENTIFIC_DATA
  return []
}

/**
 * Get pillar statistics
 */
export function getPillarStats() {
  return {
    technical: {
      total: MOCK_TECHNICAL_DATA.length,
      pending: MOCK_TECHNICAL_DATA.filter(d => d.status === 'pending').length,
      completed: MOCK_TECHNICAL_DATA.filter(d => d.status === 'completed').length,
      avgQuality: MOCK_TECHNICAL_DATA.reduce((sum, d) => sum + (d.qualityScore || 0), 0) / MOCK_TECHNICAL_DATA.length
    },
    psychological: {
      total: MOCK_PSYCHOLOGICAL_DATA.length,
      pending: MOCK_PSYCHOLOGICAL_DATA.filter(d => d.status === 'pending').length,
      completed: MOCK_PSYCHOLOGICAL_DATA.filter(d => d.status === 'completed').length,
      avgQuality: MOCK_PSYCHOLOGICAL_DATA.reduce((sum, d) => sum + (d.qualityScore || 0), 0) / MOCK_PSYCHOLOGICAL_DATA.length
    },
    scientific: {
      total: MOCK_SCIENTIFIC_DATA.length,
      pending: MOCK_SCIENTIFIC_DATA.filter(d => d.status === 'pending').length,
      completed: MOCK_SCIENTIFIC_DATA.filter(d => d.status === 'completed').length,
      avgQuality: MOCK_SCIENTIFIC_DATA.reduce((sum, d) => sum + (d.qualityScore || 0), 0) / MOCK_SCIENTIFIC_DATA.length
    }
  }
}
